		<tipoMensaje>PRUEBA_PETICION</tipoMensaje>
		
		<!-- Fecha del mensaje -->
		<fecha>2019-09-24</fecha>
		
		<!-- Hora de envio -->
		<horaEnvio>11:00:00</horaEnvio>
		
		<!-- Hora de recepción -->
		<horaRecepcion>11:00:01</horaRecepcion>
		
	</cabecera>
	
