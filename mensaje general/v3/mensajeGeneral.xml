 xsi:noNamespaceSchemaLocation="mensajeGeneral.xsd">

	<!-- CABECERA -->
	<cabecera>
	
		<!-- Identificador del mensaje -->
		<idMensaje>1234</idMensaje>
		
		<!-- Identificador del emisor (dividido en IP e ID) -->
		<identificacionEmisor>
			<ipEmisor>aaaaa</ipEmisor>
			<idEmisor>1</idEmisor>
		</identificacionEmisor>
		
